  {
   "cell_type": "code",
   "execution_count": 34,
   "id": "religious-objective",
   "metadata": {},
   "outputs": [],
   "source": [
    "def sigmoid(x):\n",
    "    return 1/ (1 + np.exp(-x))\n",
    "\n",
    "def train(X, y, iterrations, lr):\n",
    "    m, n = X.shape \n",
    "    weights = np.zeros((n, 1))\n",
    "    bias = 0 \n",
    "    for it in range(iterrations + 1):\n",
    "        y_predict = sigmoid(np.dot(X, weights) + bias)\n",
    "        loss = (-1/m * np.sum(y * np.log(y_predict) + (1-y) * np.log(1 - y_predict)))\n",
    "        \n",
    "        dw = 1 / m * np.dot(X.T, (y_predict - y))\n",
    "        db = 1/m * np.sum(y_predict - y)\n",
    "        \n",
    "        weights -= lr * dw\n",
    "        bias -= lr * db\n",
    "        \n",
    "        if it % 1000 == 0:\n",
    "            print(f'Loss after iterations{it} is {loss}')\n",
    "    return weights, bias\n",
    "        \n",
    "        "
   ]
  },
